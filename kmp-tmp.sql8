INSERT INTO Kompozytor VALUES ('Rychlik.J', 'Rychlik', 'Józef', '1946', '', '<notka>
   studiował kompozycję (u Bogusława Schaeffera<index>Schaeffer
   Bogusław</index>) oraz teorię muzyki w Państwowej Wyższej Szkole
   Muzycznej w Krakowie. W 1978 r.  uczestniczył w kursach kompozycji
   elektroakustycznej Groupe de Recherches Musicales przy Radio
   France. Przygotował i prowadził kursu muzyki elektronicznej podczas
   II, III i V Festiwalu „Młodzi Muzycy Młodemu Miastu” w Stalowej
   Woli. Wykłada w Akademii Muzycznej w Krakowie; jest autorem prac
   z dziedziny teorii muzyki. Otrzymał II nagrodę na XIV Konkursie
   Młodych Kompozytorów ZKP za <title>Sekwencje przestrzenne</title>,
   III nagrodę na Międzynarodowym Konkursie Kompozytorskim na utwór
   organowy w Szczecinie za <title>Grave</title> oraz wyróżnienie na
   IX Międzynarodowym Konkursie Muzyki Elektroakustycznej w Bourges za
   <title>Podtytuł – Sen Eurydyki</title>.
  </notka><wzk><wkompozycje>
   <wk><title>Muzyka symfoniczna I i II</title> (1969)</wk>
   <wk><title>Sekwencje przestrzenne</title> na orkiestrę kameralną (1971)</wk>
   <wk><title>Grave</title> na organy (1973)</wk>
   <wk><title>Peut-etre</title>, kompozycja graficzna na 1–7 dowolnych wykonawców z użyciem środków elektronicznych (1974)</wk>
   <wk><title>Accialto</title> na altówkę i orkiestrę kameralną (1975)</wk>
   <wk><title>Podtytuł – Sen Eurydyki</title> na taśmę (1978)</wk>
  </wkompozycje></wzk>', '', '', '');
