INSERT INTO Kompozytor VALUES ('Sowa.A', 'Sowa', 'Aleksandra', '', '', '<notka>studentka klasy kompozycji Akademii Muzycznej w Łodzi.</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Stalmierski.J', 'Stalmierski', 'Janusz', '1959', '', '<notka>ukończył klasę kompozycji Andrzeja Koszewskiego w Akademii Muzycznej
w Poznaniu; współtwórca Pomorskiej Grupy kompozytorów, od 1982 roku związany
ze szczecińską  filią AM w Poznaniu. Obecnie prowadzi klasę kompozycji 
w swojej macierzystej uczelni oraz wykłada w AM w Bydgoszczy.</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Stępień.J', 'Stępień z d.Markowska', 'Jolanta ', '1955', '', '<notka>
   studiowała teorię  muzyki (1973–1978) oraz kompozycję  u Bernarda
   Pietrzaka<index>Pietrzak Bernard</index> i Bronisława Kazimierza
   Przybylskiego<index>Przybylski Bronisław Kazimierz</index>
   (1975–1980) w Państwowej Wyższej Szkole Muzycznej
   w Łodzi. Uczestniczy a w międzynarodowym seminarium poświęconym
   Z. Kodalyowi w Kecskemet oraz warsztatach kompozytorskich w Wenecji
   k/Żnina i Wzdowie. W latach 1981–1983 pełniła funkcję  wiceprzewodniczącej
   Zarządu Głównego Koła Młodych przy ZKP; działała w Zarządzie Łódzkiego
Oddziału Stowarzyszenia polskiej Młodzieży Muzycznej Jeunesses Musicales.
Rozwijała swój warsztat kompozytorski pod kierunkiem Larsa Gunnara Bodina,
Szablocsa Esztenyi i Mariana Borkowskiego. Jest pedagogiem Ogólnokształcącej Szkoły
Muzycznej w Łodzi.
  </notka><wzk><wkompozycje>
   <wk><title>Wariacje I</title> na fortepian (1974)</wk>
   <wk><title>Akwatyki</title> na smyczki i instrumenty d te (1974)</wk>
   <wk><title>Sonata</title> na fortepian (1975)</wk>
   <wk><title>Melorecytacje</title> na mezzosopran skrzypce, altówk , wiolonczel  i fortepian (1977)</wk>
   <wk><title>Fluktuacje</title> na kwartet smyczkowy (1978)</wk>
   <wk><title>Etiuda sonorystyczna</title> dla 3 perkusistów (1979)</wk>
   <wk><title>Koncert</title> na trzy orkiestry kameralne (1980)</wk>
   <wk><title>Mouvement</title> na flet i kwartet smyczkowy (1980)</wk>
   <wk><title>Trio</title> na skrzypce, fortepian i perkusję  (1980)</wk>
   <wk><title>Impresje</title> na baryton, altówk  i fortepian (1983)</wk>
  </wkompozycje></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Stępniewska.K', 'Stępniewska', 'Katarzyna', '1950', '', '<notka>
   ukończyła studia w zakresie teorii muzyki i kompozycji w klasie
   Bogusława Schaeffera<index>Schaeffer Bogusław</index> w krakowskiej
