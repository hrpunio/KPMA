INSERT INTO Kompozytor VALUES ('Bonk.R', 'Bonk', 'Remigiusz', '', '', '<notka>kompozytor, aranżer, pedagog w Prywatnej Szkole Muzycznej
„Veos Studio” w Zabrzu. Współpracuje z Teatrem Rozrywki w Chorzowie.</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Bortnowski.M', 'Bortnowski', 'Marcin', '1972', '', '<notka>
   absolwent PSM II st. w Zielonej Górze w klasie akordeonu Stanisława
   Wasiaka. W latach 1992–1997 studiował w Akademii Muzycznej we
   Wrocławiu w klasie kompozycji Grażyny
   Pstrokońskiej-Nawratil<index>Pstrokońska-Nawratil Grażyna</index>
   (dyplom z wyróżnieniem). W 1996 r. uczestniczył w Międzynarodowych
   Spotkaniach Młodych Kompozytorów w Apeldoorn, organizowanych prze
   Fundację Gaudeamus, a 1997 r. brał udział w warsztatach muzyki
   eksperymentalnej i komputerowej w Gandawie. Jest laureatem kilku
   konkursów kompozytorskich: II nagroda za <title>Kwartet
   smyczkowy</title> oraz nagroda specjalna za <title>II kwartet
   smyczkowy</title> na Konkursie Młodych Kompozytorów im. T. Bairda
   (1995), III nagroda za <title>Symfonię</title> na wielką orkiestrę
   symfoniczną na Ogólnopolskim konkursie Kompozytorskim Gdańsk 1997,
   II nagroda za <title>Music for...</title> na dwa akordeony
   i perkusję na Konkursie Młodych Kompozytorów im. T. Bairda (1997),
   I nagroda za <title>Music in Lent</title> na akordeon i kwartet
   smyczkowy na Międzynarodowym Konkursie Kompozytorskim „Pan
   Accordion 2000” (Finlandia).  Jego utwory wykonywane były w kraju
   i za granicą.  Obecnie pracuje jako asystent na Wydziale
   kompozycji, Dyrygentury, Teorii Muzyki i Muzykoterapii Akademii
   Muzycznej we Wrocławiu. Pełni funkcję sekretarza Oddziału
   Wrocławskiego Związku Kompozytorów Polskich.
  </notka><wzk><wkompozycje>
   <wk><title>I Kwartet Smyczkowy</title> (1994)</wk>
   <wk><title>II Kwartet Smyczkowy</title> (1995)</wk>
   <wk><title>Agnus Dei</title> na mezzosopran, chór i orkiestrę symfoniczną (1995)</wk>
   <wk><title>Music in Autumn</title> na orkiestrę kameralną (1996)</wk>
   <wk><title>Symfonia nr 1</title> na wielką orkiestrę symfoniczną (1997)</wk>
   <wk><title>Music in Spring</title> na kwintet fortepianowy (1998)</wk>
   <wk><title>Symfonia nr 2</title> na wielką orkiestrę symfoniczną (1999)</wk>
   <wk><title>White Angels</title> na 13 instrumentów smyczkowych (2001)</wk>
  </wkompozycje></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Botor.H', 'Botor', 'Henryk Jan', '1960', '', '<notka>kompozytor i organista; ukończył studia w Akademii Muzycznej
  w Krakowie w zakresie wychowania muzycznego, kompozycji (klasa Marka
  Stachowskiego) oraz gry na organach (klasa Mirosławy
  Semeniuk-Podraza).  Koncertował jako organista w Polsce, Holandii
  i Niemczech wykonując swoje utwory i improwizacje. Jest laureatem
  konkursów kompozytorskich. Pracuje jako organista w krakowskich
  kościołach, uczy improwizacji fortepianowej i organowej
  w macierzystej uczelni. 
  </notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Buchwald.M', 'Buchwald', 'Magdalena', '1972', '', '<notka>studiowała muzykologię na uniwersytecie Adama Mickiewicza
  w Poznaniu, a następnie kompozycję pod kierunkiem Andrzeja
  Koszewskiego i Lidii Zielińskiej w poznańskiej Akademii
  Muzycznej. Odbyła studia uzupełniające w Stuttgarcie i Frankfurcie
  n. Menem; brała udział w wielu kursach kompozytorskich. Jej utwory
  wykonywane były na festiwalach w Polsce oraz za granicą (Niemcy,
  Francja, USA). Jest członkiem Niemieckiego Towarzystwa Muzyki
  Elektroakustycznej.</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Buczek.B', 'Buczek', 'Barbara', '1940', '1993', '<notka>
   studiowała grę na fortepianie u Ludwika
   Stefańskiego<index>Stefański Ludwik</index> i kompozycję
   u Bogusława Schaeffera<index>Schaeffer Bogusław</index>
   w Państwowej Wyższej Szkole Muzycznej w Krakowie. Jest laureatką
   nagród i wyróżnień na konkursach kompozytorskich w Katowicach,
   Warszawie, Łańcucie, Rzymie i Wiedniu.  W 1984 r. uczestniczyła
   w Międzynarodowym Seminarium Kompozytorskim w Boswil. Jej utwory
   były wykonywane na festiwalach muzyki współczesnej w kraju i za
   granicą. Jest autorką studiów i artykułów o muzyce
   współczesnej. Należała do Stowarzyszenia „Grupa Krakowska” oraz
   międzynarodowego zrzeszenia „Frau und Musik”.
  </notka><wzk><wkompozycje>
   <wk><title>Sonata breve</title> na fortepian (1968)</wk>
   <wk><title>Mikrosonata</title> na skrzypce (1968)</wk>
   <wk><title>Kwintet dęty</title> (1969)</wk>
   <wk><title>Koncert wokalny</title> na 12 głosów solowych (1969)</wk>
   <wk><title>Dwie impresje</title> na orkiestrę (1970)</wk>
   <wk><title>Anekumena</title>, koncert dla 89 wykonawców (1974)</wk>
   <wk><title>Simplex</title> na orkiestrę (1976)</wk>
   <wk><title>Eidos I</title> na skrzypce (1977)</wk>
   <wk><title>Koncert na skrzypce i orkiestrę</title> (1979)</wk>
   <wk><title>Transgressio</title> na kwartet smyczkowy (1985)</wk>
   <wk><title>Primus inter pares</title> na róg i 6 instrumentów (1985)</wk>
  </wkompozycje></wzk>', '', '', '');
