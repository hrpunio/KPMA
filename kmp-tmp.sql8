INSERT INTO Kompozytor VALUES ('Sarnecki.P', 'Sarnecki', 'Piotr', '', '', '<notka>absolwent klasy kompozycji Akademii Muzycznej w Krakowie. Jego
muzyka była wykonywana na Międzynarodowych Dniach Muzyki Kompozytorów
Krakowskich.</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Sarwas.J', 'Sarwas', 'Jakub', '1977', '', '<notka>studiował kompozycję pod kierunkiem Edwarda Bogusławskiego
  w Akademii Muzycznej w Katowicach oraz w Wyższej Szkole Muzycznej
  w Kolonii. Był uczestnikiem wielu kursów kompozytorskich w Polsce i
  za granicą; laureat konkursów, wielokrotny stypendysta. Jest
  założycielem Sarwas Project i Sarwas Ensemble, z którymi
  współpracuje jako kompozytor, dyrygent i instrumentalista.
</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Sawa.M', 'Sawa', 'Marian', '1937', '2005', '<notka>
   studiował grę na organach pod kierunkiem Feliksa Rączkowskiego
   w Państwowej Wyższej Szkole Muzycznej w Warszawie (dyplom w 1963 r.)  
   oraz kompozycję u Kazimierza Sikorskiego (dyplom w 1968 r.). 
Jako solista i akompaniator współpracował z Zespołem
   Kameralistów Filharmonii Narodowej, Warszawską Operą Kameralną,
   Teatrem Wielkim. Ma na swym koncie nagrania radiowe, telewizyjne
   i płytowe. Wykładał w warszawskiej Akademii Muzycznej oraz
   w Akademii Teologii Katolickiej. Jest laureatem I nagrody
   w Konkursie Młodych ZKP w roku 1969.
  </notka><wzk><wkompozycje>
   <wk><title>Toccata</title> na organy (1967)</wk>
   <wk><title>Symfonia koncertująca</title> na organy i orkiestrę (1968)</wk>
   <wk><title>Passacaglia</title> na organy (1971)</wk>
   <wk><title>Dyptyk Warszawski</title> na organy (1978)</wk>
   <wk><title>Requiem dla uciśnionych</title> (1980)</wk>
   <wk><title>Jutrznia</title> na organy (1985)</wk>
   <wk><title>Koncert na organy solo</title> (1990)</wk>
  </wkompozycje></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Schaeffer.B', 'Schaeffer', 'Bogusław', '1929', '', '<notka>
   studiował w krakowskiej Państwowej Wyższej Szkole Muzycznej –
   kompozycję pod kierunkiem Artura Malawskiego<index>Malawski
   Artur</index> i muzykologię u Zdzisława
   Jachimeckiego<index>Jachimecki Zdzisław</index>.  Jest doktorem
   nauk humanistycznych, autorem wielu książek na temat nowej
   muzyki. Jego dorobek obejmuje ponad 300 kompozycji, a także 17
   sztuk teatralnych, które grane były w wielu miastach Polski i za
   granicą (m.in. Hamburg, Wiedeń, Madryt, Tallin). Jest także
   grafikiem (wystawy m.in. w Wiedniu, Kolonii, Paryżu). Otrzymał
   wiele nagród i wyróżnień za kompozycje orkiestrowe
   i kameralne. W 1987 r. na Festiwalu Polskich Sztuk Współczesnych we
   Wrocławiu otrzymał nagrodę za twórczość dramaturgiczną.
   W 1990 r. założył w Salzburgu własne wydawnictwo muzyczne Collsch
   Edition.
  </notka><wzk><wkompozycje>
   <wk><title>Uwertura Warszawska</title> (1975)</wk>
   <wk><title>Romuald Traugutt</title> (1976)</wk>
   <wk><title>Missa electronica</title> na chór chłopięcy i taśmę (1975)</wk>
   <wk><title>Model X</title> na fortepian (1977)</wk>
   <wk><title>blues III</title> na 2 fortepiany (1978)</wk>
   <wk><title>Kongruencje</title> na wibrafon i fortepian (1980)</wk>
   <wk><title>Euphony</title> na głos i kontrabas dla kontrabasistki-mezzosopranu (1981)</wk>
   <wk><title>Koncert gitarowy</title> (1984)</wk>
   <wk><title>4 Sonaty organowe</title> (1985–1986)</wk>
   <wk><title>V Kwartet smyczkowy</title> (1986)</wk>
   <wk><title>Koncert na perkusję jazzową, fortepian i orkiestrę</title> (1988)</wk>
   <wk><title>III Koncert fortepianowy</title>, wersja z mediami elektronicznymi, taśmą i komputerem (1991)</wk>
  </wkompozycje></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Sielicki.E', 'Sielicki', 'Edward', '1956', '', '<notka>
   studiował kompozycję u Andrzeja Dobrowolskiego i Włodzimierza
   Kotońskiego<index>Kotoński Włodzimierz</index> w Państwowej Wyższej
   Szkole Muzycznej w Warszawie.  Studia uzupełniające w zakresie
   kompozycji i muzyki elektronicznej odbył w Konserwatorium
   w Amsterdamie u Tona de Leeuw<index>Leeuw@de Leeuw
   Ton</index>. Otrzymał wyróżnienie na XXIV Konkursie Młodych ZKP za
   <title>Psalmy błagalne</title> oraz II nagrodę na Międzynarodowym
   Konkursie Kompozytorskim w Genewie za <title>Luxuria</title>. Jego
   utwory wykonywane były w kraju i za granicą (m.in. w Meksyku, USA,
   Niemczech, Belgii i we Włoszech).
  </notka><wzk><wkompozycje>
   <wk><title>Movimento concertante</title> na saksofon altowy i orkiestrę (1977–1978)</wk>
   <wk><title>Agnus Dei</title> na chór mieszany a cappella (1978), <title>Luxuria</title> na orkiestrę (1978–1979)</wk>
   <wk><title>Lasciate mi vibrare</title> dla 3 perkusistów (1979)</wk>
   <wk><title>Ecclesiastes</title>, oratorium na głosy solowe, chór i orkiestrę (1979)</wk>
   <wk><title>Shir Takhnunim</title> na chór męski, instrumenty dęte blaszane i perkusję (1981)</wk>
   <wk><title>Salve Regina</title> na chór chłopięcy i organy (1981)</wk>
   <wk><title>Kwartet smyczkowy</title> (1982–1983)</wk>
   <wk><title>Kwartet obojowy</title> (1984)</wk>
   <wk><title>Kronos</title> na kontrabas solo (1984)</wk>
   <wk><title>Due canoni con intermezzo e finale</title> na orkiestrę smyczkową (1985)</wk>
   <wk><title>Cadenza per corno inglese</title> (1986)</wk>
   <wk><title>Arpeggione per violino e chitarra</title> (1986)</wk>
   <wk><title>Dulci sono melodiae...</title> na baryton i organy (1987)</wk>
   <wk><title>Diferencias</title> na zespół instrumentów dawnych (1988)</wk>
   <wk><title>ZIG-ZAG</title> na perkusję i taśmę (1988)</wk>
   <wk><title>Tabulatura</title> na organy solo (1988)</wk>
  </wkompozycje></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Skrzypczak.B', 'Skrzypczak', 'Bettina', '1963', '', '<notka>studiowała kompozycję u Andrzeja Koszewskiego oraz teorię
  muzyki w Akademii Muzycznej w Poznaniu. Od 1988 roku mieszka
  w Szwajcarii, gdzie kontynuowała swoją edukację muzyczną. Aktualnie
  pracuje w wyższej Szkole Muzycznej w Lucernie na stanowisku profesora. 
  Prowadziła kursy kompozytorskie w Boswil, Warszawie
  i Darmstadt. Uhonorowana wieloma nagrodami za swoją twórczość oraz
  działalność kulturalną.</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Słowik.Z', 'Słowik', 'Zbigniew', '1972', '', '<notka>kompozytor i aranżer; studiował pod kierunkiem Zbigniewa
  Bagińskiego w Akademii Muzycznej im. F.Chopina w Warszawie. Jest
  laureatem wielu konkursów kompozytorskich. Pracuje jako nauczyciel
  przedmiotów ogólnomuzycznych w PSM I i II st. w Żyrardowie, dyrygent
  chórów, organista. Jego utwory zostały nagrane dla wytwórni Acte
  Prealable.
</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Słowiński.W', 'Słowiński', 'Władysław', '1930', '', '<notka>
   studiował kompozycję u Tadeusza Szeligowskiego<index>Szeligowski
   Tadeusz</index> oraz dyrygenturę u Waleriana
   Bierdiajewa<index>Bierdiajew Walerian</index> i Stanisława
   Wisłockiego<index>Wisłocki Stanisław</index> w Państwowej Wyższej
   Szkole Muzycznej w Poznaniu, a ponadto muzykologię pod kierunkiem
   Adolfa Chybińskiego<index>Chubiński Adolf</index> na Uniwersytecie
   Poznańskiem.  W latach 1954–1964 był dyrygentem w Operze
   Poznańskiej, później zaś jako dyrygent współpracował z różnymi
   orkiestrami i teatrami muzycznymi, m.in. Teatrem Wielkim w Łodzi
   i Teatrem Wielkim w Warszawie. Od 1970 r. do 1973 r. był
   kierownikiem artystycznym Polskich Nagrań. W latach 1973–1985
   piastował funkcję sekretarza generalnego Związku Kompozytorów
   Polskich. W 1977 r. otrzymał Nagrodę Prezesa Rady Ministrów za
   twórczość dla dzieci i młodzieży, a w 1990 r. został laureatem
   nagrody Wojewody Warszawskiego za osiągnięcia twórcze w dziedzinie
   muzyki.
  </notka><wzk><wkompozycje>
   <wk><title>Sonatina</title> na fortepian (1951)</wk>
   <wk><title>Kwintet na instrumenty dęte</title> (1957)</wk>
   <wk><title>Muzyka w trzech częściach</title> na orkiestrę (1958)</wk>
   <wk><title>Dialogi</title> na skrzypce i klawesyn (1964)</wk>
   <wk><title>Cyrk Mistrza Cho-Cho</title>, balet (1964)</wk>
   <wk><title>Muzyczne drobiazgi</title> na fortepian (1978)</wk>
   <wk><title>Passionato per quattro archi</title> (1987)</wk>
   <wk><title>Concertino</title> na klawesyn i kwartet smyczkowy (1988)</wk>
   <wk><title>Tren</title> na saksofon altowy i organy (1988)</wk>
   <wk><title>Serio e buffo per flauto solo</title> (1989)</wk>
   <wk><title>Dwie pieśni</title> na głos i organy (1990)</wk>
   <wk><title>Espressivo sempre per flauto e quartetto d’archi</title> (1990)</wk>
   <wk><title>Pieśni staropolskie</title> na tenor i orkiestrę smyczkową (1991)</wk>
  </wkompozycje></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Smolska.J', 'Smolska', 'Jolanta', '1946', '', '<notka>ukończyła studia w zakresie teorii muzyki w klasie Franciszka
Wesołowskiego oraz studia kompozytorskie w klasie Tadeusza Kiesewettera 
w Państwowej Wyższej Szkole Muzycznej w Łodzi. Jest wykładowcą w macierzystej
uczelni. Jej utwory prezentowane były na festiwalach „Poznańska Wiosna 
Muzyczna, Łódzka Wiosna Artystyczna, Musica Moderna. Jest także autorką 
utworów dla dzieci i młodzieży.</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Sotomski.J', 'Sotomski', 'Jacek', '1987', '', '<notka>student klasy kompozycji Agaty Zubel w Akademii Muzycznej 
