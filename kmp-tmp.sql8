INSERT INTO Kompozytor VALUES ('Rudziński.Z', 'Rudziński', 'Zbigniew', '1935', '', '<notka>
   studia kompozytorskie odbył pod kierunkiem Piotra
   Perkowskiego<index>Perkowski Piotr</index> w Państwowej Wyższej
   Szkole Muzycznej w Warszawie, uzupełniał je w Paryżu u Nadii
   Boulanger<index>Boulanger Nadia</index>.  Jest laureatem V Konkursu
   Młodych Kompozytorów ZKP oraz konkursu kompozytorskiego z okazji
   150-lecia Konserwatorium Warszawskiego (1960). Wykłada kompozycję
