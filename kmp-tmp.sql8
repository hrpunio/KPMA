INSERT INTO Kompozytor VALUES ('Gordiejuk.M', 'Gordiejuk', 'Marian', '1954', '2005', '<notka>
   studiował teorię muzyki u Franciszka Wesołowskiego
   <index>Wesołowski Franciszek</index> i kompozycję u Jerzego
   Bauera<index>Bauer Jerzy</index> w Wyższej Szkole Muzycznej
   w Łodzi. Od 1977 r. pracował jako wykładowca przedmiotów
   teoretycznych w Akademii Muzycznej w Bydgoszczy. Jest laureatem
   konkursów kompozytorskich.
  </notka><wzk><wkompozycje>
   <wk><title>Toccatina</title> na orkiestrę symfoniczną (1977)</wk>
   <wk><title>Kwartet perkusyjny</title> (1978)</wk>
   <wk><title>Kwartet smyczkowy</title> (1980)</wk>
