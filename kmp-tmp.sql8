-- Generowanie katalogu w formacie SQL ................... --
-- Wykorzystanie:--
-- mysql -u <user> -p < kmp-tmp.sql8 --
DROP database if exists kpma;
CREATE database kpma CHARACTER SET utf8 COLLATE utf8_bin;
use kpma;
DROP TABLE if exists Kompozytor;
DROP TABLE if exists Utwor;
DROP TABLE if exists UtworKomp;
-- kompozytorzy --
CREATE TABLE Kompozytor (
    id CHAR(40),
    nazwisko CHAR(30),
    imie CHAR(20),
    ur INTEGER,
    zm INTEGER,
    info_pl TEXT,
    info_en TEXT,
    email CHAR(40),
    zdjecie CHAR(15));
-- utwory --
-- 2004-8-9: zmiana nazw dwoch ostatnich pol --
CREATE TABLE Utwor (
    id INTEGER,
    tytul TEXT,
    krotki_tytul TEXT,
    rok INTEGER,
    id_wydawnictwo TEXT,
    sklad TEXT,
    xsklad TEXT,
    typ CHAR(18),
    nagranie CHAR(18),
    zrodlo TEXT  ); 
CREATE TABLE UtworKomp (
    idu INTEGER,
    idk CHAR(40)); 

-- Kompozytorzy--
INSERT INTO Kompozytor VALUES ('Augustyn.R', 'Augustyn', 'Rafał', '1951', '', '<notka>
   studiował kompozycję w Państwowej Wyższej Szkole Muzycznej we
   Wrocławiu w klasie Ryszarda Bukowskiego<index>Bukowski
    Ryszard</index> oraz w Państwowej Wyższej Szkole Muzycznej
   w Katowicach pod kierunkiem Henryka Mikołaja
   Góreckiego<index>Górecki Henryk Mikołaj</index>. 
   Ukończył również
   studia polonistyczne na Uniwersytecie Wrocławskim. Zajmuje się
   krytyką i teorią muzyki. Jest laureatem konkursów kompozytorskich
   (m.in. Międzynarodowego Konkursu Organowego w Szczecinie i XXII
   Konkursu Młodych ZKP w Warszawie – II nagroda za
   <title>Atlantydę I</title>.
  </notka><wzk><wkompozycje>
   <wk>2 <title>kwartety smyczkowe</title> (1972, 1981)</wk>
   <wk><title>Monosonata</title> na fortepian (1976)</wk>
   <wk><title>Ballada</title> na orkiestrę smyczkową (1977)</wk>
   <wk><title>Atlantyda I</title> na wielką orkiestrę (1979)</wk>
   <wk><title>Klangfarbenmelodie</title> na kwartet perkusyjny (1979)</wk>
   <wk><title>En blanc et noir</title> na klawesyn (1979)</wk>
   <wk><title>Romans</title> na puzon, kotły i dwa fortepiany (1980)</wk>
   <wk><title>Carmina de tempore</title> na sopran, fortepian i altówkę (1981)</wk>
   <wk><title>Atlantyda II</title> na orkiestrę i chór (1983)</wk>
   <wk><title>A Life’s Parallels</title> na głos wysoki i orkiestrę (1983)</wk>
   <wk><title>Long Island Rail Road...</title> na skrzypce i przedmioty towarzyszące (1984)</wk>
   <wk><title>Figle szatana</title>, balet (1984–1985)</wk>
   <wk><title>Utwór cykliczny</title> na skrzypce solo (1986)</wk>
   <wk><title>Varesiana</title> na flet solo (1987)</wk>
   <wk><title>Król siedmiodniowy</title>, pantomima (1988)</wk>
   <wk><title>Wariacje na temat Paganiniego</title> na fortepian (1987–1989)</wk>
  </wkompozycje></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Baculewski.K', 'Baculewski', 'Krzysztof', '1950', '', '<notka>
   absolwent Państwowej Wyższej Szkoły Muzycznej w Warszawie
   (1974). Studia uzupełniał w konserwatorium paryskim u Oliviera
   Messiaena i w Groupe de Recherches Musicales. Współpracuje jako
   krytyk z „Ruchem Muzycznym”. Jego utwory wykonywane były w kraju
   i za granicą (RFN, Francja, Węgry, Finlandia, USA, Ameryka
   Płd.). Jest laureatem kilku konkursów kompozytorskich oraz autorem
   pracy doktorskiej napisanej w Instytucie Muzykologii Uniwersytetu
   Warszawskiego pod kierunkiem Józefa Chomińskiego, poświęconej
   warsztatowi kompozytorskiemu współczesnych polskich twórców.
  </notka><wzk><wkompozycje>
   <wk><title>Sonata</title> na perkusję solo (1972)</wk>
   <wk><title>Meander</title> na flet solo (1973)</wk>
   <wk><title>Epitafium</title> na orkiestrę (1974)</wk>
   <wk><title>Nowe Wyzwolenie</title>, dramat muzyczny (1974)</wk>
   <wk><title>Vivace e Cantilena</title> na flet, fortepian i kwintet smyczkowy (1974)</wk>
   <wk><title>La notte</title>, kantat kameralna (1975)</wk>
   <wk><title>Trois Graces</title> na organy (1975)</wk>
   <wk><title>Is-slottet</title> na 12 instrumentów (1975)</wk>
   <wk><title>A la recherche des harmonies perdues</title> na orkiestrę (1976–1977)</wk>
   <wk><title>Passacaglia</title> na kwartet perkusyjny (1979)</wk>
   <wk><title>Partita</title> na saksofon altowy i klawesyn (1980)</wk>
   <wk><title>Ground</title> na orkiestrę (1981)</wk>
   <wk><title>Quartier Latin</title> na taśmę (1981)</wk>
   <wk><title>Sonata wiosenna</title> na flet solo (1982)</wk>
   <wk><title>Koncert na orkiestrę</title> (1983), 3 kwartety smyczkowe (1984, 1985, 1986)</wk>
   <wk><title>The Whole and Broken Consort</title> na zespół instrumentów dawnych (1986)</wk>
   <wk><title>Concerto armonico</title> na orkiestrę smyczkową (1987)</wk>
   <wk><title>Partita</title> na skrzypce i fortepian (1988)</wk>
   <wk><title>A Walking Shadow</title> na małą orkiestrę symfoniczną (1990)</wk>
  </wkompozycje></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Bargielski.Z', 'Bargielski', 'Zbigniew', '1937', '', '<notka>
   studiował prawo na Uniwersytecie im. Marii Skłodowskiej-Curie
   w Lublinie. W 1958 r. rozpoczął studia kompozytorskie w Państwowej
   Wyższej Szkole Muzycznej w Warszawie pod kierunkiem Tadeusza
   Szeligowskiego<index>Szeligowski Tadeusz</index>, a po jego śmierci
   kontynuował je u Bolesława Szabelskiego<index>Szabelski
   Bolesław</index> w Państwowej Wyższej Szkole Muzycznej
   w Katowicach.  W latach 1966–1967 odbywał studia uzupełniające
   u Nadii Boulanger<index>Boulanger Nadia</index> w Paryżu,
   a w 1972 r. -- w Hochschule für Musik w Grazu. Jest laureatem
   licznych nagród na konkursach kompozytorskich, m.in. wyróżnienie na
   Międzynarodowej Trybunie Kompozytorów UNESCO w Paryżu w 1981 r.
   Jego utwory wykonywane były w wielu krajach Europy, w Stanach
   Zjednoczonych, w Australii i Ameryce Południowej. Obok kompozycji
   uprawia publicystykę i pedagogikę muzyczną. Obecnie mieszka
   w Wiedniu.
  </notka><wzk><wkompozycje>
   <wk><title>Danton</title>, opera wg G. Büchnera (1968–1969)</wk>
   <wk><title>Ein Zimmer</title> na zespół kameralny (1972)</wk>
   <wk><title>Koncert na perkusję i orkiestrę</title> (1975)</wk>
   <wk><title>Koncert na skrzypce i orkiestrę</title> (1976)</wk>
   <wk><title>Kwartet smyczkowy „Alpejski”</title> (1976)</wk>
   <wk><title>Klatka dla motyli</title> na kwintet dęty (1978)</wk>
   <wk><title>W małym dworku</title>, opera (1979)</wk>
   <wk><title>II Kwartet smyczkowy „Wiosenny”</title> (1980)</wk>
   <wk><title>Historyjki wiedeńskie</title> na dwa fortepiany (1981)</wk>
   <wk><title>Sonnenlieder</title> na głosy, chór i zespół kameralny (1983)</wk>
   <wk><title>Wyprawa w niewysłowioność</title> na orkiestrę (1986)</wk>
   <wk><title>III kwartet smyczkowy „Martwa natura z krzykiem”</title> (1985-86)</wk>
   <wk><title>Ziemia niczyja</title>, oratorium (1989)</wk>
   <wk><title>Cha-ord</title> na orkiestrę (1990)</wk>
   <wk><title>Concertino</title> na fortepian i orkiestrę (1995)</wk>
   <wk><title>Tango</title> na zespół kameralny (1997)</wk>
  </wkompozycje></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Bauer.J', 'Bauer', 'Jerzy', '1936', '', '<notka>
   studiował w Państwowej Wyższej Szkole Muzycznej w Łodzi: teorię
   muzyki u Franciszka Wesołowskiego<index>Wesołowski
   Franciszek</index>, dyrygenturę i kompozycję u Tomasza
   Kiesewettera<index>Kiesewetter Tomasz</index>. W latach 1969–1970
   uzupełniał studia kompozytorskie u Nadii Boulanger<index>Boulanger
   Nadia</index> w Paryżu. Wykłada w Akademii Muzycznej w Łodzi. Jest
   laureatem kilkunastu krajowych konkursów
   kompozytorskich. W 1989 r. zdobył brązowy medal na XXXI
   Międzynarodowym Konkursie Radio France na kompozycję gitarową.
  </notka><wzk><wkompozycje>
   <wk><title>Rzeźby średniowiecza</title> na chór mieszany, chór chłopięcy i wielką orkiestrę (1970–1971)</wk>
   <wk><title>Pomnik krzyczący</title> na chór mieszany i wielką orkiestrę (1972)</wk>
   <wk><title>Nike Warszawska</title> na orkiestrę (1974)</wk>
   <wk><title>Concerto per violoncello e orchestra</title> (1984)</wk>
   <wk><title>Jasne świergotania</title> na flet solo i orkiestrę (1987)</wk>
  </wkompozycje></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Blecharz.W', 'Blecharz', 'Wojciech', '1981', '', '<notka>ur. w Gdyni, absolwent klasy oboju OSM II st. w Gdańsku,
  ukończył z wyróżnieniem studia kompozytorskie pod kierunkiem Mariana
  Borkowskiego w AMFC w Warszawie. Od 2008 roku student studiów
  doktoranckich na Uniwersytecie w San Diego (USA). </notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Błażejczyk.W', 'Błażejczyk', 'Wojciech', '', '', '<notka>kompozytor, gitarzysta, realizator dźwięku, aranżer;
  absolwent Wydziału Reżyserii Dźwięku, klasy kompozycji Zygmunta
  Krauzego UMFC w Warszawie oraz Wydziału Dziennikarstwa i Nauk
  Politycznych Uniwersytetu Warszawskiego. Na gitarze elektrycznej
  wykonuje muzykę improwizowaną, eksperymentalną, elektroakustyczną 
  i współczesną. Komponuje dla filmu i teatru; nagrywa muzykę poważną
  i rozrywkową, współpracuje jako realizator dźwięku z festiwalem
  „Warszawska Jesień”.</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Błażewicz.M', 'Błażewicz', 'Marcin', '1953', '', '<notka>kompozytor i animator życia muzycznego, pedagog; studiował
  filozofię w Akademii Teologii Katolickiej w Warszawie oraz
  kompozycję w klasie Mariana Borkowskiego w AMFC w Warszawie. Pełnił
  wiele funkcji, m.in prezesa polskiej sekcji Jeunesses Musicales,
  wiceprezesa Towarzystwa Sztuki Perkusyjnej, kierownika muzycznego
  Teatru Polskiego Radia, założyciela i redaktora naczelnego
  czasopisma „Muzyka 21”. Od 2004 roku prowadzi klasę kompozycji
  w AMFC w Warszawie, pełnił funkcję prodziekana i Dyrektora Instytutu
  Kompozycji w macierzystej uczelni.
</notka><wzk></wzk>', '', '', '');
INSERT INTO Kompozytor VALUES ('Bogusławski.E', 'Bogusławski', 'Edward', '1940', '2003', '<notka>
